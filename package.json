{
  "name": "coding-assignment-evaluator",
  "version": "1.0.0",
  "description": "Coding assignment evaluator",
  "author": "Vincas Stonys <vincas.stonys@gmail.com>",
  "main": "Sources/index.js",
  "private": false,
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9",
    "yarn": ">=0.17.9"
  },
  "scripts": {
    "start": "cd front && webpack-dev-server --hot --inline --progress --history-api-fallback",
    "server-debug": "set DEBUG=coding-assignment-evaluator-api:* && yarn start-server",
    "start-server": "cd api && gulp serve",
    "build": "cd api && gulp",
    "test": "cd api && cross-env NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "test:watch": "cd api && yarn test -- --watch",
    "test:check-coverage": "cd api && yarn test && istanbul check-coverage",
    "report-coverage": "cd api && coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vincaslt/coding-assignment-evaluator"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.22.0",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "codemirror": "^5.23.0",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "humanize-duration": "^3.10.0",
    "joi": "10.0.6",
    "jsonwebtoken": "7.1.9",
    "method-override": "^2.3.5",
    "mongoose": "4.7.4",
    "morgan": "1.7.0",
    "react": "^15.4.1",
    "react-codemirror": "^0.3.0",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "^0.14.1",
    "reselect": "^2.5.4",
    "sanitize.css": "^4.1.0",
    "uuid": "^3.0.1",
    "vm2": "^3.1.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "6.13.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.12.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "2.1.14",
    "ghooks": "^1.2.4",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "isparta": "4.0.0",
    "json-loader": "^0.5.4",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "style-loader": "^0.13.1",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "stage-2",
      "stage-3",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  }
}
